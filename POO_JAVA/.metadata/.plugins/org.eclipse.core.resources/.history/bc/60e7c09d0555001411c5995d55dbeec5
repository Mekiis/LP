package TP02;

public class Main {
	
	public static void main(String[] args) {
		// Time : 9.X s
		//Long sum = 0L;
		// Time : 1.X s
		long sum = 0L;
		long startTime = System.currentTimeMillis();
		for (long i = 0; i < Integer.MAX_VALUE; i++) {
			sum += i; 
		}
		System.out.println((System.currentTimeMillis() - startTime) / 1000f);
    }
	
}
